{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "object-curly-newline": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "operator-linebreak": "off"
  }
}
